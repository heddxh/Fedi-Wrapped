{
  "name": "fedi-wrapped",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "GEMINI_API_KEY=$(grep GEMINI_API_KEY .env.local | cut -d '=' -f2) node server/index.js",
